# E2E Test Configuration Example
# Copy to .env in root directory and customize

# Base URLs
# Local development (výchozí):
PRE_BASE_URL=https://core-platform.local
POST_BASE_URL=https://core-platform.local

# Staging/Production:
# POST_BASE_URL=https://staging.your-domain.com

# TLS Validation
E2E_IGNORE_TLS=true

# Test Users (defaults are set in code)
# E2E_USER=test
# E2E_PASS=Test.1234
# E2E_ADMIN_USER=test_admin
# E2E_ADMIN_PASS=Test.1234

# Keycloak Configuration (optional overrides)
# E2E_REALM=admin
# E2E_CLIENT_ID=web
