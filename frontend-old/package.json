{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist && mkdir -p dist",
    "build": "node esbuild.mjs",
    "preview": "npx serve dist -l 5173",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "dev": "npm run build && npm run docker:rebuild && npm run watch",
    "watch": "chokidar \"src/**/*\" -c \"npm run build && npm run docker:rebuild\"",
    "docker:rebuild": "cd ../docker && docker-compose up -d --build --no-deps frontend nginx"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@iconify/react": "^6.0.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/lab": "^7.0.0-beta.12",
    "@mui/material": "^7.1.0",
    "@mui/system": "^7.1.0",
    "@mui/x-date-pickers": "^7.22.2",
    "@tabler/icons-react": "^3.33.0",
    "apexcharts": "^4.7.0",
    "dayjs": "^1.11.13",
    "keycloak-js": "^24.0.4",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-dom": "18.3.1",
    "react-helmet": "^6.1.0",
    "react-mui-sidebar": "^1.6.3",
    "react-router": "6.28.0",
    "react-router-dom": "6.28.0",
    "simplebar-react": "^3.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@eslint/js": "^9.33.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-jest": "^29.7.0",
    "chokidar-cli": "^3.0.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.33.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  }
}
