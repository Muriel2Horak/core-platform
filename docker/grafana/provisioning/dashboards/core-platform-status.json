              },
              "type": "regex"
            },
            {
              "options": {
                "pattern": ".*warn.*",
                "result": {
                  "color": "yellow",
                  "index": 1
                }
              },
              "type": "regex"
            },
            {
              "options": {
                "pattern": ".*starting.*|.*ready.*",
                "result": {
                  "color": "green",
                  "index": 2
                }
              },
              "type": "regex"
            }
          ],
          "thresholds": {
            "mode": "absolute",
